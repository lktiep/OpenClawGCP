# OpenClaw Gateway Configuration
# Copy this to .env and fill in your values

# Required
OPENCLAW_GATEWAY_TOKEN=your_gateway_token
OPENCLAW_CONFIG_DIR=/home/tieple/.openclaw
OPENCLAW_WORKSPACE_DIR=/home/tieple/.openclaw/workspace

# LLM Provider
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_BASE_URL=https://api.anthropic.com/v1
OPENCLAW_AGENT_MODEL=claude-opus-4-5-thinking

# Optional - R2 Storage
R2_ACCESS_KEY_ID=your_r2_key
R2_SECRET_ACCESS_KEY=your_r2_secret

# Optional - MCP Servers
KEOTHOM_MCP_API_KEY=your_keothom_mcp_key

# Optional - Claude Web Session
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# Optional - Gateway
OPENCLAW_GATEWAY_PORT=18789
OPENCLAW_BRIDGE_PORT=18790
OPENCLAW_GATEWAY_BIND=lan
